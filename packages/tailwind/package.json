{
  "name": "@lockerai/tailwind",
  "version": "0.1.0",
  "repository": "https://github.com/nitic-pbl-p8/lockerai.git",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint './**/{*.*js,*.*ts}'",
