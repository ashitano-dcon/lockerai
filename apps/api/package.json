{
  "name": "@lockerai/api",
  "version": "0.1.0",
  "repository": "https://github.com/cardo-lockerai/lockerai.git",
  "author": "dino3616 <85730998+dino3616@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "type": "commonjs",
  "module": "nodenext",
  "scripts": {
    "generate": "pnpm db:generate",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start",
    "prod": "cross-env NODE_ENV=production node dist/main",
    "prod:dev": "cross-env NODE_ENV=development node dist/main",
    "lint": "eslint './**/{*.*js,*.*ts}'",
    "lint:fix": "eslint './**/{*.*js,*.*ts}' --fix",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "test": "cross-env NODE_ENV=test jest --silent=false --verbose false --passWithNoTests",
    "db:generate": "prisma generate --schema ./src/infra/prisma/schema.prisma",
    "db:migrate": "dotenv -e .env.production -- cross-env NODE_ENV=production prisma migrate dev --schema ./src/infra/prisma/schema.prisma --name new_migration",
    "db:deploy:dev": "dotenv -e .env.development -- cross-env NODE_ENV=development prisma migrate deploy --schema ./src/infra/prisma/schema.prisma",
    "db:deploy:prod": "dotenv -e .env.production -- cross-env NODE_ENV=production prisma migrate deploy --schema ./src/infra/prisma/schema.prisma",
    "db:studio": "dotenv -e .env.development -- cross-env NODE_ENV=development prisma studio --schema ./src/infra/prisma/schema.prisma",
    "apollo:rover": "rover graph publish lockerai@current --schema ./schema.gql"
  },
  "dependencies": {
    "@apollo/server": "4.9.5",
    "@nestjs/apollo": "12.0.9",
    "@nestjs/cli": "10.2.1",
    "@nestjs/common": "10.2.7",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.7",
    "@nestjs/graphql": "12.0.9",
    "@nestjs/platform-express": "10.2.7",
    "@nestjs/testing": "10.2.7",
    "@prisma/client": "5.6.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dataloader": "2.2.2",
    "dotenv": "16.3.1",
    "drizzle-orm": "0.28.6",
    "graphql-subscriptions": "2.0.0",
    "graphql-validation-complexity": "0.4.2",
    "postgres": "3.4.2",
    "ts-pattern": "5.0.5",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@apollo/rover": "0.21.0",
    "@lockerai/jest": "workspace:*",
    "@lockerai/postcss": "workspace:*",
    "@lockerai/prettier": "workspace:*",
    "@lockerai/stylelint": "workspace:*",
    "@lockerai/tsconfig": "workspace:*",
    "@lockerai/type": "workspace:*",
    "@nestjs/schematics": "10.0.3",
    "@types/graphql-validation-complexity": "0.4.3",
    "@types/pg": "8.10.7",
    "drizzle-kit": "0.19.13",
    "eslint-config-lockerai-node": "workspace:*",
    "pg": "8.11.3",
    "prisma": "5.6.0"
  }
}
