    "@lockerai/tsconfig": "workspace:*",
    "@lockerai/type": "workspace:*",
    "add": "2.0.6",
    "autoprefixer": "10.4.16",
    "eslint-config-lockerai-esm": "workspace:*",
    "mini-svg-data-uri": "1.4.4",
    "postcss": "8.4.33",
    "postcss-import": "16.0.0",
    "tailwindcss": "3.4.1",
