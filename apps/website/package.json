    "@lockerai/design-token": "workspace:*",
    "@lockerai/tailwind": "workspace:*",
    "@lockerai/urql": "workspace:*",
    "@supabase/auth-helpers-nextjs": "0.10.0",
    "@supabase/auth-helpers-react": "0.5.0",
    "@supabase/supabase-js": "2.49.4",
    "@urql/next": "1.1.0",
    "ai": "4.3.9",
    "dotenv": "16.3.1",
    "framer-motion": "10.18.0",
    "graphql-tag": "2.12.6",
    "marked": "15.0.8",
